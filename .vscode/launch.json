{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug TypeScript (node)",
      "type": "pwa-node",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "node",
      "args": ["--enable-source-maps","${workspaceFolder}/src/index.ts"],
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "skipFiles": ["<node_internals>/**"]
    }
  ]
}
